<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springmvc_mybatis.mapper.CodeConfigMapper">
    <resultMap type="CodeConfig" id="codeConfigResult">
        <result column="dbid" property="dbid"/>
        <result column="dmbh" property="dmbh"/>
        <result column="code" property="code"/>
        <result column="content" property="content"/>
    </resultMap>
    <select id="queryAllCode" resultMap="codeConfigResult">
    select * from be3.code_config
	</select>
</mapper>