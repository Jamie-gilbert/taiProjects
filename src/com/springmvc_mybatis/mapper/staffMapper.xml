        <result column="XMPY" property="XMPY"/>

    </resultMap>
    <select id="queryStaff" resultMap="staffResult">
SELECT * FROM (
  SELECT A.* ,ROWNUM RN FROM
		(select * from
		si3.per_reg
		where  cbrylb =#{cbrylb} AND  DWID =#{dwid}
		) A
		WHERE ROWNUM &lt;= #{next}
		) WHERE RN >=#{pre}

</select>
</mapper>
